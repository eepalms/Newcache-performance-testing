    return
}

run_zeroxbench_test() {
    am start -a android.intent.action.MAIN -n org.zeroxlab.zeroxbenchmark/com.nea.nehe.lesson16.Run
    return
}

sleep 2
/sbin/m5 checkpoint
/sbin/m5 dumpstats 10000000000000 10000000000000
/sbin/m5 resetstats 
run_zeroxbench_test

