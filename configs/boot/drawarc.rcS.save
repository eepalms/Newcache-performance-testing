#!/system/bin/sh

# Author: Anthony Gutierrez
# Modified: Bijie Qiu on 6/25/2013
# 0xbench-2D rendering-DrawArc

stop_m5() {
    /sbin/m5 exit
    return
}

run_zeroxbench_test() {
    am start -a android.intent.action.MAIN -n org.zeroxlab.zeroxbenchmark/org.zeroxlab.graphics.DrawArc
    return
}
sleep 10
/sbin/m5 checkpoint
/sbin/m5 dumpstats
/sbin/m5 resetstats
run_zeroxbench_test
wait
stop_m5
